#include <iostream>
using namespace std;

class Student
{
	// These member variables can be accessed by anyfunction.
	public:
		int studId;
		string studName;
		
		// Default constructor : invoked during the object creation without passing any arguments
		Student()
		{
			// initialized in constructor 
			studId = 0;
			studName = "";
		}
		
};

int main()
{
	// Creating a student object
	Student s1;
	Student s2;
	
	cout << s1.studId << " " << s1.studName << "\n\n";
	
	s2.studId = 1234;
	s2.studName = "Jane";
	
	cout << s2.studId << " " << s2.studName << "\n\n";
	
	
	
	return 0;
}
